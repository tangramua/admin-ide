{
  "name": "adminide-stack-workspace-server",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "watch": "pm2-runtime run start:dev",
    "start": "pm2-runtime dist/index.js",
    "start:staging": "cross-env NODE_ENV=staging ENV_FILE=../../config/staging/staging.env nodemon dist",
    "start:dev": "cross-env ENV_FILE=../../config/development/dev.env nodemon dist",
    "docker:build": "npm run build && docker build . -t $npm_package_name:$npm_package_version",
    "docker:run": "docker run -it --env-file ../../config/staging/docker-staging.env  $npm_package_name:$npm_package_version",
    "prebuild": "npm run build:clean",
    "pretest": "npm run build",
    "build": "cross-env NODE_ENV=production npm run build:dist",
    "build:watch": "npm run build:dist -- --watch",
    "build:dist": "webpack",
    "build:clean": "rimraf dist",
    "jest": "./node_modules/.bin/jest",
    "test": "jest",
    "test:debug": "npm test -- --runInBand",
    "test:watch": "npm test -- --watch"
  },
  "author": "CDMBase LLC",
  "license": "ISC",
  "dependencies": {
    "@adminide-stack/core": "file:../../packages/adminide-core",
    "@adminide-stack/workspaces-server": "file:../../packages-modules/workspaces/server",
    "@cdm-logger/server": "^5.0.5",
    "@common-stack/core": "0.0.213",
    "@common-stack/server-core": "0.0.213",
    "@common-stack/store-mongo": "0.0.213",
    "apollo-client": "^2.6.4",
    "dotenv": "^7.0.0",
    "envalid": "^5.0.0",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "graphql-nats-subscriptions": "^1.3.0",
    "helmet": "^3.16.0",
    "hemera-joi": "^6.0.2",
    "hemera-plugin": "2.0.1",
    "hemera-safe-promises": "^4.0.1",
    "hemera-sql-store": "^7.0.0",
    "hemera-zipkin": "^6.0.5",
    "inversify": "^5.0.1",
    "ioredis": "^4.14.0",
    "mongoose": "^5.1.4",
    "nats": "^1.2.10",
    "nats-hemera": "^7.0.1",
    "react": "^16.10.2",
    "reflect-metadata": "^0.1.13",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "pm2": "^3.5.0",
    "rimraf": "^2.6.3"
  }
}
